{
    "name": "jmleroux/pdf-merger",
    "description": "PDF File Merger for PHP 7",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Paul Clegg",
            "email": "paul@clegginabox.co.uk"
        },
        {
            "name": "JM Leroux",
            "email": "jmleroux.pro@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.2",
        "setasign/fpdi": "^2.3",
        "setasign/fpdf": "^1.8",
        "tecnickcom/tcpdf": "^6.3",
        "setasign/tfpdf": "^1.31"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5|^9.1",
        "friendsofphp/php-cs-fixer": "^2.16"
    },
    "autoload": {
        "psr-4": {
            "Jmleroux\\PDFMerger\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jmleroux\\Tests\\": "tests/"
        }
